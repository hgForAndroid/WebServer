@model List<WebServer.Models.Document.Document>
@{
    Layout = "~/Views/Shared/_Layout_organizor.cshtml";
    ViewBag.Title = "中佰会议系统组织者";
}

@section BodyLess{
    <script src="~/js/common/jquery/jquery.flash.js" type="text/javascript"></script>
    <script src="~/js/common/jquery/jquery.jquploader.js" type="text/javascript"></script>
    <script src="~/js/common/jquery/jquery-1.5.1.min.js" type="text/javascript"></script>
    <link href="~/less/document/add.less" type="text/css" rel="stylesheet" />
}

<div>
    <input type="hidden" id="agendaID" class="agendaID" value="@Session["agendaID"]" />
    <input type="hidden" id="meetingID" class="meetingID" value="@Session["meetingID"]" />
</div>

<div class="container" id="mainContent">
    <div class="panel panel-default" id="mainContentRight">
        <div>
            <div class="col-md-6 ">
                <h2><strong>新增议程附件</strong></h2>
            </div>
            <div class="col-md-4">
            </div>
            <div class="col-md-2 ">
                <h3>
                    <span id="ContentRightHeadState">
                        状态:<span id="Status"></span>
                    </span>
                </h3>
            </div>
            
        </div>
        <hr style="FILTER: alpha(opacity=100,finishopacity=0,style=3)" width="100%" color=#987cb9 size=3>
        <div id="newAppendix">
            <input type="file" id="file_upload" name="file" />
            <div style="background:#848484;width:100px;height:10px;margin-top:5px">
                <div id="progressNumber" style="background:#428bca;width:0px;height:10px">
                </div>
            </div>
            <font id="percent">0%</font>   
        </div>
        <div class="row">
            <div class="col-md-2 col-md-offset-3"></div>
            <div class="col-md-4 col-md-offset-3" id="buttonGroup">
                <button class="btn btn-default returnIndex" type="button" >返回</button>&nbsp;&nbsp;
                <button class="btn btn-default upload" type="button" >上传</button>
            </div>
        </div>
    </div>
</div>

<script src="~/js/document/add.js" type="text/javascript" ></script>

