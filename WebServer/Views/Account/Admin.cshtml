@model Tuple<int,int,int,int>
@{
    Layout = "~/Views/Shared/_Layout_admin.cshtml";
    ViewBag.Title = "中佰会议系统管理员";
}
@section BodyLess{
    <link href="~/less/account/admin.less" type="text/css" rel="stylesheet">
}

<div class="panel col-md-9 col-md-offset-1" id="mainContentRight">
    <div>
        <div class="panel panel-default col-md-5" id="meetingPlace">
            会场：
            <div class="buttonGropSingle">
                <button type="button" class="btn btn-button btn-default btn-lg">
                    会场总计:@Model.Item1
                </button>
            </div>
        </div>
        <div class="panel panel-default col-md-5 col-md-offset-2" id="device">
            设备：
            <div class="buttonGropSingle">
                <button type="button" class="btn btn-button btn-default btn-lg">
                    设备总计:@Model.Item2
                </button>
            </div>
        </div>
    </div>
    <div>
        <div class="panel panel-default col-md-5" id="user">
            人员：
            <div class="buttonGropSingle">
                <button type="button" class="btn btn-button btn-default btn-lg">
                    人员总计:@Model.Item3
                </button>
            </div>
        </div>
        <div class="panel panel-default col-md-5 col-md-offset-2" id="role">
            角色：
            <div class="buttonGropSingle">
                <button type="button" class="btn btn-button btn-default btn-lg">
                    角色总计:@Model.Item4
                </button>
            </div>
        </div>
    </div>
</div>

<script src="~/js/account/admin.js" type="text/javascript"></script>