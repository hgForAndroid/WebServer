@{
    Layout = "~/Views/Shared/_Layout_admin.cshtml";
    ViewBag.Title = "中佰会议系统管理员";
}
@section BodyLess{
    <link href="~/less/user/edit.less" type="text/css" rel="stylesheet">
}


    <div class="panel panel-default col-md-9 col-md-offset-1" id="mainContentRight">
        <div>
            <div class="col-md-3 ">
                <h1><strong>编辑人员</strong></h1>
            </div>
            <div class="col-md-6">
                <br />
                <ol class="breadcrumb">
                    <li><a href="/Account/Admin">首页</a></li>
                    <li><a href="/User/Index_admin">人员管理</a></li>
                    <li class="active">编辑人员</li>
                </ol>
            </div>
            <div class="col-md-3">
                <br />
                <button class="btn btn-default" type="button" id="cancel1" href="#">取消编辑</button>
            </div>
            <div class="row">
                <div class="col-md-8"></div>
                <div class="col-md-4" id="ContentRightHeadState">
                    状态:<span id="Status"></span>
                </div>
            </div>
        </div>

        <div id="inputEditPeople">
            <p id="editPeople1"><strong>姓名：</strong><input id="input1" type="text" name="editPeople"></p>
            <p id="editPeople2"><strong>部门单位：</strong><input id="input2" type="text" name="editPeople"></p>
            <p id="editPeople3"><strong>职务：</strong><input id="input3" type="text" name="editPeople"></p>
            <div>
                <strong>角色：</strong>
                <select name="select" id="select" class="xla_k"></select>
            </div>


            <div><strong>详细描述：</strong></div><textarea id="description"></textarea>

        </div>


        <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4 col-md-offset-4" id="cancel2Keep">
                <button class="btn btn-default" type="button" id="cancel2" href="#">取消编辑</button>&nbsp;&nbsp;
                <button class="btn btn-default" type="button" id="keep" href="#">保存编辑</button>
            </div>
        </div>
    </div>
<script src="~/js/user/edit.js"></script>
