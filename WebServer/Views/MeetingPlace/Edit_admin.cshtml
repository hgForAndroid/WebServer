@{
    Layout = "~/Views/Shared/_Layout_admin.cshtml";
    ViewBag.Title = "中佰会议系统管理员";
}
@section BodyLess{
    <link href="~/less/meetingPlace/edit.less" type="text/css" rel="stylesheet">
}
        <div class="panel panel-default col-md-9 col-md-offset-1" id="mainContentRight">
            <div>
                <div class="col-md-3 ">
                    <h1><strong>编辑会场</strong></h1>
                </div>
                <div class="col-md-7">
                    <br />
                    <ol class="breadcrumb">
                        <li><a href="/Account/Admin">首页</a></li>
                        <li><a href="/MeetingPlace/Index_admin">会场管理</a></li>
                        <li class="active">编辑会场</li>
                    </ol>
                </div>
                <div class="col-md-2 ">
                    <br />
                    <button class="btn btn-default" type="button" id="cancel1" href="#">取 消</button>
                </div>
                <div class="row">
                    <div class="col-md-9"></div>
                    <div class="col-md-3" id="ContentRightHeadState">
                        状态:<span id="Status"></span>
                    </div>
                </div>
            </div>
            <div>
                <p id="editMeeting1">
                    <strong>会场名称：</strong>
                    <input id="input1" type="text" name="editMeeting">
                </p>
                <p id="editMeeting2">
                    <strong>可容纳的人数：</strong>
                    <input id="input2" type="number" name="editMeeting">
                </p>
                <div id="editMeetingselect">
                    <strong>会场座位类型：</strong>
                    <select name="select" id="select">
                        <option value="1">长矩形桌</option>
                    </select>
                </div>
                <p id="editMeeting3">
                    <strong>上座位数：</strong>
                    <input id="input3" type="number" name="addMeeting">
                </p>
                <p id="editMeeting4">
                    <strong>下座位数：</strong>
                    <input id="input4" type="number" name="addMeeting">
                </p>
                <p id="editMeeting5">
                    <strong>左座位数：</strong>
                    <input id="input5" type="number" name="addMeeting">
                </p>
                <p id="editMeeting6">
                    <strong>右座位数：</strong>
                    <input id="input6" type="number" name="addMeeting">
                </p>
            </div>

            <div class="row">
                <div class="col-md-4"></div>
                <div class="col-md-4 col-md-offset-4" id="cancel2Keep">
                    <button class="btn btn-default" type="button" id="cancel2" href="#">取 消</button>&nbsp;&nbsp;
                    <button class="btn btn-default" type="button" id="keep" href="#">保存编辑</button>
                </div>
            </div>
        </div>

        <script src="~/js/meetingPlace/edit.js"></script>