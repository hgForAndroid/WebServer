    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.5" />
    
    
    <authentication mode="Forms">
      <forms name="LoginCookie" loginUrl="/Account/Index" defaultUrl="/Account/Index" protection="All" timeout="30" path="/" requireSSL="false" slidingExpiration="false" enableCrossAppRedirects="false" cookieless="UseDeviceProfile" domain="" />
    </authentication>

